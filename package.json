{
  "name": "pino-grigio",
  "version": "0.0.0",
  "description": "🍷 A Clean and developer friendly transport for Pino logger 🌲.",
  "keywords": [
    "pino",
    "transport",
    "develop",
    "debug",
    "logger"
  ],
  "author": {
    "name": "Brandon Pierce",
    "url": "https://brandonjpierce.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "example-http": "node examples/http.js | node bin.js",
    "example-hapi": "node examples/hapi.js | node bin.js",
    "example-express": "DEBUG=* node -r pino-debug examples/express.js | node bin.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "main": "./index.js",
  "bin": {
    "pino-grigio": "./bin.js"
  },
  "dependencies": {
    "boxen": "^1.1.0",
    "chalk": "^1.1.3",
    "clean-stack": "^1.2.0",
    "fast-json-parse": "^1.0.2",
    "parse-headers": "^2.0.1",
    "pretty-bytes": "^4.0.2",
    "pretty-ms": "^2.1.0",
    "pump": "^1.0.2",
    "split2": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.1.0",
    "express": "^4.15.3",
    "express-pino-logger": "^2.0.0",
    "hapi": "^16.1.1",
    "hapi-pino": "^1.4.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.0",
    "pino": "^4.5.2",
    "pino-debug": "^1.0.3",
    "pino-http": "^2.6.1",
    "prettier": "^1.3.1"
  }
}
